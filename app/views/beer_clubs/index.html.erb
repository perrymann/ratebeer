<p id="notice"><%= notice %></p>

<h1>Beer Clubs</h1>
<table class="table table-hover">
  <tbody>
  <% @beer_clubs.each do |beer_club| %>
  <tr>
      <td><%= link_to beer_club.name, beer_club %></td>
  </tr>
  <%end%>
  </tbody>
</table>
<div><%= link_to 'New Beer club', new_beer_club_path, class: 'btn btn-info btn-xs' if current_user%></div>